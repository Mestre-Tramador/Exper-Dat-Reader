{
  "name": "mestre-tramador/exper-dat-reader",
  "description": "Read .dat files easily and store their dumped data.",
  "version": "0.0.1",
  "keywords": [
    "exper",
    "lumen",
    "dat",
    "study",
    "php",
    "php8.0"
  ],
  "license": "MIT",
  "type": "system",
  "authors": [
    {
      "name": "Mestre-Tramador",
      "homepage": "https://github.com/Mestre-Tramador"
    }
  ],
  "require": {
    "php": "^8.0",
    "illuminate/support": "^9.24",
    "laravel/lumen-framework": "^9.0",
    "league/flysystem": "^3.2",
    "tomfordrumm/jwt-auth": "dev-develop"
  },
  "require-dev": {
    "fakerphp/faker": "^1.9.1",
    "mockery/mockery": "^1.4.4",
    "phpunit/phpunit": "^9.5.10"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\": "database/seeders/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
